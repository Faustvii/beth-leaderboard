{
  "name": "test",
  "module": "src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run --hot src/main.ts\" \"bun tw:dev\"",
    "start": "concurrently \"bun tw\" \"bun run src/main.ts\"",
    "db:push": "bunx drizzle-kit push:sqlite",
    "db:studio": "bunx drizzle-kit studio",
    "db:seed": "bun run src/db/seed.ts",
    "db:generate": "bunx drizzle-kit generate:sqlite",
    "tw": "bunx tailwindcss --minify -i ./src/styles.css -o ./public/styles.css",
    "tw:dev": "bunx tailwindcss -i ./src/styles.css -o ./public/styles.css --watch",
    "typecheck": "bunx --bun tsc",
    "format:check": "prettier --check .",
    "format": "prettier --write . --list-different",
    "lint": "eslint . --report-unused-disable-directives",
    "lint:fix": "eslint . --report-unused-disable-directives --fix"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@kitajs/ts-html-plugin": "^1.2.0",
    "@types/eslint": "^8.44.2",
    "@types/web": "^0.0.115",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "better-sqlite3": "^8.6.0",
    "bun-types": "latest",
    "concurrently": "^8.2.1",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-isaacscript": "^3.5.5",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "tailwindcss": "^3.3.3",
    "chart.js": "^4.4.0",
    "@types/chart.js": "^2.9.38",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@elysiajs/cron": "^0.7.0",
    "@elysiajs/static": "^0.7.1",
    "@kitajs/html": "^3.0.3",
    "@libsql/client": "0.4.0-pre.5",
    "@lucia-auth/adapter-sqlite": "^2.0.0",
    "@lucia-auth/oauth": "^3.3.1",
    "@t3-oss/env-core": "^0.7.0",
    "@tlscipher/holt": "1.1.1",
    "clsx": "^2.0.0",
    "drizzle-orm": "^0.28.6",
    "drizzle-typebox": "^0.1.1",
    "elysia": "0.7.16",
    "lucia": "^2.7.1",
    "sharp": "0.33.0-alpha.9",
    "tailwind-merge": "^1.14.0",
    "zod": "3.22.2"
  }
}
