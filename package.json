{
  "name": "test",
  "module": "src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run --hot src/main.ts\" \"bun tw:dev\"",
    "build": "bun build ./src/main.ts --minify --target bun --splitting --outdir ./build",
    "start": "concurrently \"bun tw\" \"bun run src/main.ts\"",
    "db:studio": "bun drizzle-kit studio",
    "db:generate": "bun drizzle-kit generate",
    "tw": "bun tailwindcss --minify -i ./src/styles.css -o ./public/styles.css",
    "tw:dev": "bun tailwindcss -i ./src/styles.css -o ./public/styles.css --watch",
    "typecheck": "bun --bun tsc",
    "format:check": "prettier --check .",
    "format": "prettier --write . --list-different",
    "lint": "eslint . --report-unused-disable-directives",
    "lint:fix": "eslint . --report-unused-disable-directives --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@kitajs/ts-html-plugin": "^1.2.0",
    "@types/chart.js": "^2.9.38",
    "@types/eslint": "^9.6.1",
    "@types/web": "^0.0.115",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "bun-types": "latest",
    "chart.js": "^4.4.0",
    "concurrently": "^8.2.1",
    "drizzle-kit": "0.21.4",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.4.2",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.33.1"
  },
  "dependencies": {
    "@elysiajs/cron": "^0.7.0",
    "@elysiajs/static": "^0.7.1",
    "@kitajs/html": "^3.0.3",
    "@libsql/client": "0.4.0-pre.5",
    "@lucia-auth/adapter-sqlite": "^3.0.2",
    "@lucia-auth/oauth": "^3.3.1",
    "@t3-oss/env-core": "^0.7.0",
    "@tlscipher/holt": "1.1.1",
    "clsx": "^2.0.0",
    "drizzle-orm": "^0.44.2",
    "drizzle-typebox": "^0.3.3",
    "elysia": "0.7.30",
    "lucia": "^2.7.1",
    "openskill": "^3.1.0",
    "sharp": "0.33.0-alpha.9",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "zod": "3.22.2"
  }
}
